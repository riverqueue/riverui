    "preview": "LIVE_FS=true reflex -c .reflex.server",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "watch:frontend": "vite --open http://localhost:8080/",
    "watch:backend": "LIVE_FS=true DEV=true reflex -c .reflex.server"
