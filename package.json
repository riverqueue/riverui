  },
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "dev": "concurrently --kill-others \"npm:watch:*\"",
    "fmt": "eslint --cache --fix --report-unused-disable-directives --max-warnings 0 . && prettier --cache --log-level error --write .",
